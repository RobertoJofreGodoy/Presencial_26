<h1>LIST</h1>
<div class="row">
    <div class="col-md-6">
            <h1> Completed TRUE</h1>
            <table class="table table-well">
            <thead>
                <tr>
                    <th>Description </th>
                    <th> Completed</th>
                    <th colspan="3"> </th>
                </tr>
            </thead>
            <tbody>
            <% @todo_complete_true.each do |todo| %>
                <tr>
                    <td> <%= todo.description %></td>
                    <td> <%= todo.completed%></td>
                    <td> <%= link_to ' SHOW', todo, :class => 'btn btn-primary'%> </td>
                    <td> <%= link_to ' EDIT', edit_path(todo),:class => 'btn btn-primary' %> </td>
                    <td> <%= link_to ' COMPLETE', complete_path(todo),:class => 'btn btn-success' %> </td>
                    <td> <%= link_to ' DESTROY', todo, method: :delete,:class => 'btn btn-warning' %> </td>
                </tr>
            <%end%>
            </tbody>
        </table>
    </div>

    <div class="col-md-6">
    <h1> Completed FALSE</h1>
                <table class="table table-well">
            <thead>
                <tr>
                    <th>Description </th>
                    <th> Completed</th>
                    <th colspan="3"> </th>
                </tr>
            </thead>
            <tbody>
            <% @todo_complete_false.each do |todo| %>
                <tr>
                    <td> <%= todo.description %></td>
                    <td> <%= todo.completed%></td>
                    <td> <%= link_to ' SHOW', todo, :class => 'btn btn-primary'%> </td>
                    <td> <%= link_to ' EDIT', edit_path(todo),:class => 'btn btn-primary' %> </td>
                    <td> <%= link_to ' COMPLETE', complete_path(todo),:class => 'btn btn-success' %> </td>
                    <td> <%= link_to ' DESTROY', todo, method: :delete,:class => 'btn btn-warning' %> </td>
                </tr>
            <%end%>
            </tbody>
        </table>
    </div>
</div>